{
  "name": "automation-core",
  "private": true,
  "scripts": {
    "start": "meteor run",
    "debug": "meteor run --inspect",
    "test": "npm run unit-test",
    "test-i": "npm run integration-test",
    "unit-test": "TEST_WATCH=1 meteor test --driver-package meteortesting:mocha",
    "integration-test": "TEST_WATCH=1 meteor test --full-app --driver-package meteortesting:mocha",
    "test-once": "meteor test --once --driver-package meteortesting:mocha",
    "license-validate": "node-license-validator -p -d --allow-licenses MIT BSD BSD-3-Clause ISC Apache Public-Domain 'Public Domain'",
    "i18n-extract-pot": "i18next-extract-gettext --files=\"./client/ui/**/*.tsx\" --output=i18n/template.pot",
    "i18n-compile-json": "npm run i18n-compile-json-nb & npm run i18n-compile-json-sv",
    "i18n-compile-json-nb": "i18next-conv -l nb -s i18n/nb.po -t public/locales/nb/translations.json",
    "i18n-compile-json-sv": "i18next-conv -l sv -s i18n/sv.po -t public/locales/sv/translations.json"
  },
  "dependencies": {
    "@babel/runtime": "^7.0.0-beta.48",
    "@fortawesome/fontawesome": "^1.1.7",
    "@fortawesome/fontawesome-free-solid": "^5.0.13",
    "@fortawesome/react-fontawesome": "0.0.19",
    "@nrk/core-icons": "^3.2.0",
    "@types/body-parser": "^1.17.0",
    "body-parser": "^1.18.3",
    "chai": "^4.1.2",
    "classnames": "^2.2.5",
    "core-js": "^2.5.4",
    "element-resize-event": "^2.0.9",
    "fast-clone": "^1.5.3",
    "html-entities": "^1.2.1",
    "i18next": "^11.2.3",
    "i18next-browser-languagedetector": "^2.2.0",
    "i18next-xhr-backend": "^1.5.1",
    "jquery": "^3.3.1",
    "lottie-web": "^5.1.10",
    "meteor-node-stubs": "^0.3.3",
    "moment": "^2.19.1",
    "monaco-editor": "^0.13.0",
    "mos-connection": "^0.3.15",
    "ntp-client": "^0.5.3",
    "object-path": "^0.11.4",
    "prop-types": "^15.6.1",
    "query-string": "^6.0.0",
    "react": "^16.3.0",
    "react-bootstrap": "^0.32.1",
    "react-contextmenu": "^2.9.2",
    "react-dom": "^16.3.0",
    "react-escape": "0.0.8",
    "react-hotkeys": "^0.10.0",
    "react-i18next": "^7.6.0",
    "react-lottie": "^1.1.0",
    "react-moment": "^0.7.0",
    "react-monaco-editor": "^0.16.0",
    "react-router-dom": "^4.2.2",
    "react-timer-hoc": "^2.3.0",
    "safer-eval": "^1.2.3",
    "smpte-timecode": "^1.2.1",
    "soap": "^0.24.0",
    "superfly-timeline": "^2.0.3",
    "underscore": "^1.8.3",
    "velocity-animate": "^1.5.1",
    "velocity-react": "^1.3.3",
    "winston": "^2.4.2"
  },
  "devDependencies": {
    "@types/chai": "^4.1.2",
    "@types/classnames": "^2.2.3",
    "@types/css-font-loading-module": "0.0.1",
    "@types/jquery": "^3.3.1",
    "@types/meteor": "^1.4.13",
    "@types/mocha": "^2.2.48",
    "@types/mousetrap": "^1.6.0",
    "@types/node": "^9.6.1",
    "@types/prop-types": "^15.5.2",
    "@types/query-string": "^5.1.0",
    "@types/react": "^16.3.2",
    "@types/react-dom": "^16.0.4",
    "@types/react-i18next": "^7.3.2",
    "@types/react-router-dom": "^4.2.6",
    "@types/winston": "^2.3.9",
    "i18next-conv": "^6.0.0",
    "i18next-extract-gettext": "^3.2.0",
    "node-license-validator": "^1.3.0",
    "tslint": "^5.4.3",
    "tslint-config-standard": "^6.0.1",
    "typedoc": "^0.8.0",
    "typescript": "^2.8.1"
  }
}
